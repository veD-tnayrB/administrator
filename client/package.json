		"firebase": "^10.6.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"socket.io-client": "^4.5.4",
		"tippy.js": "^6.3.7"
	},
	"devDependencies": {
		"@beyond-js/local": "~0.1.3",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.9"
