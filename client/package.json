{
	"name": "@essential-js/admin",
	"version": "0.0.1",
	"title": "Administrator",
	"description": "",
	"license": "ISC",
	"layout": "auth-layout",
	"template": "template/template.json",
	"languages": {
		"default": "en",
		"supported": [
			"en",
			"es"
		]
	},
	"params": {
		"environment": "development",
		"application": {
			"localDB": "essential-dashboard",
			"localDBVersion": 1,
			"company": {
				"name": "Baleares",
				"subName": "Group"
			},
			"permissionLessRoutes": [
				"/user/profile",
				"/dashboard"
			],
			"noSessionRoutes": [
				"/auth/recover",
				"/auth/login",
				"/auth/forget-password"
			]
		},
		"server": "http://localhost:5001/admin/"
	},
	"libraries": {
		"imports": [
			"pragmate-ui"
		]
	},
	"modules": {
		"path": "modules"
	},
	"deployment": {
		"distributions": [
			{
				"name": "web",
				"environment": "development",
				"platform": "web",
				"ports": {
					"bundles": 6550
				},
				"minify": {
					"css": true,
					"js": true
				}
			},
			{
				"name": "web-tsc",
				"environment": "development",
				"platform": "web",
				"ports": {
					"bundles": 6510
				},
				"ts": {
					"compiler": "tsc"
				},
				"minify": {
					"css": true,
					"js": true
				}
			}
		]
	},
	"dependencies": {
		"@beyond-js/backend": "^0.1.9",
		"@beyond-js/bee": "^0.0.6",
		"@beyond-js/kernel": "~0.1.9",
		"@beyond-js/react-18-widgets": "~1.0.1",
		"@beyond-js/reactive": "^1.1.12",
		"@bgroup/helpers": "^1.0.7",
		"@bgroup/http-suite": "^1.0.2",
		"@firebase/app": "^0.9.23",
		"@tippyjs/react": "^4.2.6",
		"@types/dompurify": "^3.0.5",
		"@types/react-grid-layout": "^1.3.5",
		"@types/uuid": "^9.0.8",
		"chart.js": "^4.4.1",
		"date-fns": "^3.6.0",
		"dompurify": "^3.1.3",
		"firebase": "^10.6.0",
		"framer-motion": "^10.16.12",
		"http-suite": "^0.0.1",
		"idb": "^7.1.1",
		"moment-timezone": "^0.5.45",
		"npm-force-resolutions": "^0.0.10",
		"perfect-scrollbar": "^1.5.5",
		"prismjs": "^1.29.0",
		"react": "^18.2.0",
		"react-chartjs-2": "^5.2.0",
		"react-day-picker": "^8.10.0",
		"react-dom": "^18.2.0",
		"react-grid-layout": "^1.4.4",
		"react-select": "5.8.0",
		"react-toastify": "^9.1.3",
		"rrule": "^2.8.1",
		"socket.io-client": "^4.5.4",
		"swiper": "^11.1.3",
		"tippy.js": "^6.3.7"
	},
	"devDependencies": {
		"@beyond-js/local": "^0.1.3",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.9"
	},
	"resolutions": {
		"idb": "7.1.1"
	},
	"static": {
		"includes": [
			"assets",
			"firebase-messaging-sw.js",
			"vercel.json"
		]
	}
}
