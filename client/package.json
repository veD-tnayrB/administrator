{
	"name": "@essential-js/admin",
	"version": "0.0.1",
	"title": "Administrator",
	"description": "",
	"license": "ISC",
	"layout": "auth-layout",
	"template": "template/template.json",
	"languages": {
		"default": "en",
		"supported": [
			"en",
			"es"
		]
	},
	"params": {
		"application": {
			"localDB": "essential-dashboard",
			"localDBVersion": 1,
			"company": {
				"name": "Essential"
			}
		},
		"server": "http://localhost:5000/"
	},
	"libraries": {
		"imports": [
			"pragmate-ui",
			"@bgroup/http-suite"
		]
	},
	"modules": {
		"path": "modules"
	},
	"deployment": {
		"distributions": [
			{
				"name": "web",
				"environment": "development",
				"platform": "web",
				"ports": {
					"bundles": 6550
				}
			}
		]
	},
	"dependencies": {
		"@beyond-js/kernel": "~0.1.9",
		"@beyond-js/react-18-widgets": "~1.0.1",
		"@beyond-js/reactive": "^1.1.6",
		"@bgroup/helpers": "^1.0.3",
		"@bgroup/http-suite": "^1.0.0",
		"chart.js": "^4.4.1",
		"firebase": "^10.6.0",
		"framer-motion": "^10.16.12",
		"http-suite": "^0.0.1",
		"i": "^0.3.7",
		"npm": "^10.2.5",
		"react": "^18.2.0",
		"react-chartjs-2": "^5.2.0",
		"react-dom": "^18.2.0",
		"react-toastify": "^9.1.3",
		"socket.io-client": "^4.5.4",
		"tippy.js": "^6.3.7"
	},
	"devDependencies": {
		"@beyond-js/local": "~0.1.3",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.9"
	},
	"static": {
		"includes": [
			"assets"
		]
	}
}
