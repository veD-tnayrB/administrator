{
	"name": "@essential-js/admin",
	"version": "0.0.1",
	"title": "Administrator",
	"description": "",
	"license": "ISC",
	"layout": "main-admin-layout",
	"template": "template/template.json",
	"languages": {
		"default": "en",
		"supported": [
			"en",
			"es"
		]
	},
	"params": {
		"application": {
