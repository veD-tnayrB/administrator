{
	"application": {
		"processor": "sass",
		"path": "application",
		"files": [
			"application/custom-properties/definition.scss",
			"application/_variables.scss",
			"global/_variables.scss",
			"*"
		]
	},
	"global": {
		"processor": "sass",
		"path": "global",
		"files": ["*"]
	},
	"processors": {
		"scss": {
			"path": "sass",
			"files": ["*"]
		}
	},
	"overwrites": "overwrites/overwrites.json"
}
